<?xml version='1.0' encoding='UTF-8'?>
<templates>

    <template name='AEdAssembly'>
                <attribute name='controller_name_convention' type='maya.string'>
                        <label>Controller Name Convention</label>
                </attribute>

                <attribute name='joint_name_convention' type='maya.string'>
                        <label>Joint Name Convention</label>
                </attribute>

                <attribute name='controller_extension' type='maya.string'>
                        <label>Controller Extension</label>
                </attribute>

                <attribute name='joint_extension' type='maya.string'>
                        <label>Joint Extension</label>
                </attribute>

                <attribute name='side_c_str' type='maya.string'>
                        <label>Side Cstr</label>
                </attribute>

                <attribute name='side_l_str' type='maya.string'>
                        <label>Side Lstr</label>
                </attribute>

                <attribute name='side_r_str' type='maya.string'>
                        <label>Side Rstr</label>
                </attribute>
    </template>

    <view name="Rigging" template="AEdAssembly">
                <property name='controller_name_convention'/>
                <property name='joint_name_convention'/>
                <property name='controller_extension'/>
                <property name='joint_extension'/>
                <property name='side_c_str'/>
                <property name='side_l_str'/>
                <property name='side_r_str'/>
    </view>

</templates>
