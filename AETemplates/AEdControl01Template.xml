<?xml version='1.0' encoding='UTF-8'?>
<templates>

    <template name='AEdControl01'>
                <attribute name='name' type='maya.string'>
                        <label>Name</label>
                </attribute>

                <attribute name='side' type='maya.enum'>
                        <label>Side</label>
                </attribute>

                <attribute name='index' type='maya.long'>
                        <label>Index</label>
                </attribute>

                <attribute name='parent_output_index' type='maya.long'>
                        <label>Parent_output_index</label>
                </attribute>

                <attribute name='create_output_joint' type='maya.bool'>
                        <label>Create_output_joint</label>
                </attribute>

                <attribute name='mirror_type' type='maya.enum'>
                        <label>Mirror_type</label>
                </attribute>

                <attribute name='controller_count' type='maya.long'>
                        <label>Controller_count</label>
                </attribute>

                <attribute name='_parent_controller' type='maya.bool'>
                        <label>Parent Controller</label>
                </attribute>
    </template>

    <view name="Rigging" template="AEdControl01">
                <property name='name'>
                       <description language="cb">py.dsettingcb.callback_edit_name</description>
                </property>
                <property name='side'>
                       <description language="cb">py.dsettingcb.callback_edit_side</description>
                </property>
                <property name='index'>
                       <description language="cb">py.dsettingcb.callback_edit_index</description>
                </property>
                <property name='parent_output_index'>
                       <description language="cb">py.dsettingcb.callback_edit_parent_output_index</description>
                </property>
                <property name='create_output_joint'>
                       <description language="cb">py.dsettingcb.callback_toggle_create_output_joint</description>
                </property>
                <property name='mirror_type'>
                       <description language="cb">py.dsettingcb.callback_edit_mirror_type</description>
                </property>
                <property name='controller_count'>
                       <description language="cb">py.dsettingcb.callback_edit_controller_count</description>
                </property>
                <property name='_parent_controller'>
                       <description language="cb">py.dsettingcb.callback_edit_parent_controller</description>
                </property>
    </view>

</templates>
